{ 
    "manifest_version" : 2, 
    "name" : "Page Sync",
    "version" : "1.0", 
  
  
    "permissions": ["tabs", "storage"],
    "description": "Find where your partner is",
     "background" : {
       "scripts" : ["background.js"]
     },
     "content_scripts" : [
      {
      "matches" : ["<all_urls>"],
      "js" : ["content.js"],
      "run_at": "document_end"
      }
      ],
     "browser_action" : {
      "default_popup" : "popup.html"
      }
    }