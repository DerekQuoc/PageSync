{ 
    "manifest_version" : 2, 
    "name" : "Page Sync",
    "version" : "1.0", 
  
  
    "permissions": ["tabs", "storage"],
    "description": "Find where your partner is",
    "web_accessible_resources" : [ 
    "images/*.jpg", "images/*.png", "images/*.gif" 
     ],
     "content_scripts" : [
      {
      "matches" : [
      "<all_urls>"
      ],
      "js" : ["content.js"]
      }
      ],
     "browser_action" : {
      "default_popup" : "popup.html"
      }
    }